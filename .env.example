# Development Environment Configuration
# Copy this file to .env for local development

# Environment type (development, production, test)
CHERT_ENVIRONMENT=development

# Logging level (DEBUG, INFO, WARN, ERROR)
CHERT_LOG_LEVEL=DEBUG

# Network configuration
CHERT_NETWORK_ENDPOINT=http://localhost:8545

# Security settings
CHERT_SESSION_TIMEOUT=60
CHERT_AUTO_LOCK_TIMEOUT=30
CHERT_MAX_FAILED_ATTEMPTS=10

# Features
CHERT_ENABLE_ANALYTICS=false

# Development-only settings
CHERT_ENABLE_DEV_MODE=true